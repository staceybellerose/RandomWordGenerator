<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppTheme">
        <attr name="textColorPrimary" format="reference|color" />
        <attr name="textColorSecondary" format="reference|color" />
        <attr name="textColorTertiary" format="reference|color" />
    </declare-styleable>
    <!-- note: to re-use an existing Android attribute not already used by the superclass, name should have prefix "android:" and do not define a format -->
    <declare-styleable name="NumberPickerDialogPreference">
        <attr name="android:max" />
        <attr name="min" format="integer" />
        <attr name="summaryPlural" format="reference" />
    </declare-styleable>
</resources>