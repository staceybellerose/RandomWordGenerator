<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.staceybellerose.randomwordgenerator">

    <application
        android:allowBackup="true"
        android:name="com.staceybellerose.randomwordgenerator.MyApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.staceybellerose.randomwordgenerator.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.staceybellerose.randomwordgenerator.ChangelogActivity"
            android:label="@string/title_activity_changelog"
            android:parentActivityName="com.staceybellerose.randomwordgenerator.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.staceybellerose.randomwordgenerator.MainActivity" />
        </activity>
        <activity
            android:name="com.staceybellerose.randomwordgenerator.MyLibsActivity"
            android:label="@string/title_activity_about"
            android:parentActivityName="com.staceybellerose.randomwordgenerator.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.staceybellerose.randomwordgenerator.MainActivity" />
        </activity>
    </application>

</manifest>